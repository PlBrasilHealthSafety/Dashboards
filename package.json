{
  "name": "dashboards",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "dev:host": "vite --host",
    "build": "tsc -b && vite build",
    "build:analyze": "ANALYZE=true vite build",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "preview:dist": "vite preview --port 4173",
    "clean": "rm -rf dist node_modules/.vite",
    "test": "echo \"No tests configured yet\"",
    "firebase:emulators": "firebase emulators:start",
    "firebase:deploy": "firebase deploy --only firestore:rules,firestore:indexes",
    "vercel:setup": "node scripts/setup-vercel-env.js",
    "vercel:deploy": "vercel --prod",
    "vercel:preview": "vercel",
    "vercel:verify": "node scripts/verify-deployment.js",
    "preinstall": "only-allow npm"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-slot": "^1.2.3",
    "@react-three/drei": "^10.6.1",
    "@react-three/fiber": "^9.3.0",
    "@types/three": "^0.179.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "firebase": "^12.1.0",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.539.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-router-dom": "^7.8.0",
    "recharts": "^3.1.2",
    "swiper": "^11.2.10",
    "tailwind-merge": "^3.3.1",
    "three": "^0.179.1",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react-swc": "^4.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "only-allow": "^1.2.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "terser": "^5.43.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  }
}
